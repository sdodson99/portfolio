---
import type { CollectionEntry } from "astro:content";

interface Props {
  skills: CollectionEntry<"skill">[];
}

const { skills } = Astro.props;
---

<ul class="flex flex-wrap">
  {
    skills.map((skill) => (
      <li class="mr-4 my-2 block">
        <img
          alt={skill.data.name}
          height="30"
          width="30"
          src={skill.data.logoUrl}
        />
      </li>
    ))
  }
</ul>
